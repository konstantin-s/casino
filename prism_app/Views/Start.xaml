<UserControl x:Class="prism_app.Views.Start"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:local="clr-namespace:prism_app.Views"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel VerticalAlignment="Top" Background="Aquamarine">
        <TextBlock TextAlignment="Center" Background="Azure" Margin="0 40 0 0 ">Добро пожаловать в Жёванное казино</TextBlock>
        <Button Margin="0 30 0 30" Width="100" Command="{Binding Enter}">Войти</Button>
    </StackPanel>
</UserControl>